      </div>
      <div class='admin-rights-account-block'>
        <div>Pr√©nom :</div>
        <div><%= account['firstname'] %></div>
      </div>
      <div class='admin-rights-services'>
        <% for(var key in services){ %><!-- @SuppressWhitespace
        --><div id='<%= key %>' name='admin-rights-service' class='block'><%= services[key]['name'] %></div><!-- @SuppressWhitespace
      --><% } %>
      </div>
    </div>
    <% include ../../include/scripts.ejs %>
    <script src='/js/admin/rights/links.js'></script>
    <script src='/js/afk.js'></script>
