              <% } else{ %>
                <div class='rightsDetailAppsRightsFalse'><%= strings.admin.rights.detail.administrationRights.suspendAccounts %></div><% if(currentAccountRights.add_rights == 1){ %><button onclick='addRight("suspend_accounts")' name='suspendAccounts' class='rightsDetailAppsRightsButtonActivate' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleActive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } else{ %><button class='rightsDetailAppsRightsButtonInactive' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleInactive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } %>
              <% } %>
