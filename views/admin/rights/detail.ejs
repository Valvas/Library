            <% } %>
            <% if(detailedAccountRights.add_rights == 1){ %>
              <div class='rightsDetailAppsRightsTrue'><%= strings.admin.rights.detail.administrationRights.addRights %></div><% if(currentAccountRights.remove_rights == 1){ %><button name='deactivateRight' tag='addRights' class='rightsDetailAppsRightsButtonDeactivate' title='<%= strings.admin.rights.detail.administrationRights.buttons.removeRight.titleActive %>'><%= strings.admin.rights.detail.administrationRights.buttons.removeRight.content %></button><% } else{ %><button class='rightsDetailAppsRightsButtonInactive' title='<%= strings.admin.rights.detail.administrationRights.buttons.removeRight.titleInactive %>'><%= strings.admin.rights.detail.administrationRights.buttons.removeRight.content %></button><% } %>
            <% } else{ %>
              <div class='rightsDetailAppsRightsFalse'><%= strings.admin.rights.detail.administrationRights.addRights %></div><% if(currentAccountRights.add_rights == 1){ %><button name='activateRight' tag='addRights' class='rightsDetailAppsRightsButtonActivate' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleActive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } else{ %><button class='rightsDetailAppsRightsButtonInactive' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleInactive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } %>
