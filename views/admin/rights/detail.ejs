              <% } %>
            </div>
            <div id='addRights'>
              <% if(detailedAccountRights.add_rights == 1){ %>
                <div class='rightsDetailAppsRightsTrue'><%= strings.admin.rights.detail.administrationRights.addRights %></div><% if(currentAccountRights.remove_rights == 1){ %><button onclick='removeRight("add_rights")' name='addRights' class='rightsDetailAppsRightsButtonDeactivate' title='<%= strings.admin.rights.detail.administrationRights.buttons.removeRight.titleActive %>'><%= strings.admin.rights.detail.administrationRights.buttons.removeRight.content %></button><% } else{ %><button class='rightsDetailAppsRightsButtonInactive' title='<%= strings.admin.rights.detail.administrationRights.buttons.removeRight.titleInactive %>'><%= strings.admin.rights.detail.administrationRights.buttons.removeRight.content %></button><% } %>
