              <% } else{ %>
                <div class='rightsDetailAppsRightsFalse'><%= strings.admin.rights.detail.administrationRights.removeAccounts %></div><% if(currentAccountRights.add_rights == 1){ %><button onclick='addRight("remove_accounts")' name='removeAccounts' class='rightsDetailAppsRightsButtonActivate' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleActive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } else{ %><button class='rightsDetailAppsRightsButtonInactive' title='<%= strings.admin.rights.detail.administrationRights.buttons.addRight.titleInactive %>'><%= strings.admin.rights.detail.administrationRights.buttons.addRight.content %></button><% } %>
